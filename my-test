# Full block tags (with body)
namespaced_blocks = re.findall(r'<(\w+:\w+)[^>]*>[\s\S]*?</\1>', content)

# Self-closing tags
self_closing_tags = re.findall(r'<\w+:\w+[^>]*/>', content)

def extract_namespaced_jsp_tags(content):
    block_tags = re.findall(r'<(\w+:\w+)[^>]*>[\s\S]*?</\1>', content)
    self_closing_tags = re.findall(r'<\w+:\w+[^>]*/>', content)
    return block_tags + self_closing_tags
